        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <!-- 当前电脑的数据库驱动程序jar包的全路径 -->
    <classPathEntry location="G:\mavenFiles\mysql\mysql-connector-java\5.1.47\mysql-connector-java-5.1.47.jar"/>

    <context id="context" >
        <commentGenerator  >
            <property name="javaFileEncoding" value="UTF-8"/>
            <property name="suppressAllComments" value="true" />
            <property name="suppressDate" value="true"/>
        </commentGenerator>
        <!--选择数据库的driverClass与connectionURL-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/express01?useUnicode=true&amp;characterEncoding=UTF-8" userId="root" password="123" />

        <!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建  使用Maven生成在target目录下，会自动创建） -->
        <javaModelGenerator targetPackage="org.bdqn.logistics.pojo" targetProject="D:/自选项目/logistics/src/main/java" >
            <property name="enableSubPackages" value="true"/> <!-- 如果true，MBG会根据catalog和schema来生成子包。如果false就会直接用targetPackage属性。默认为false -->
            <property name="trimStrings" value="true" /> <!-- 当数据库字段使用CHAR时，建议在<javaModelGenerator>中设 -->
        </javaModelGenerator>

